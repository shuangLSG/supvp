/* 页面布局 */
.page-section-26{
    padding: 0 0.346rem;    
    margin-bottom: 0.266rem;
}
.page-section-30{
    padding: 0 .4rem;
}
.module .module-hd{
    height: 1.4rem;
    font-size:0.45rem;
    color:#222;
}
.pull-left{
    float:left;
}
.pull-right{
    float:right;
}
.cleafix{
    overflow: hidden;
    clear: both;
}
.hide,
.hide.custom-flex{
    display: none !important;
}
/*** 顶部搜索框 ***/
.custom-page-nav input[type=search]{
    border-radius: 3px;
    height: 0.853rem;
}
.custom-page-nav .mui-bar{
    padding:0 0.346rem !important;
}
.mui-bar .mui-title{
    width:100% ;
    left:0 ;
    line-height:1.1rem ;
}
.mui-bar-nav~.mui-content {
    padding-top: 1.17rem !important;
}
.mui-search .mui-icon{
    width: 0.4rem;
    height: 0.4rem;
    left: 0.32rem;
}
.mui-search .mui-placeholder{
    height: 0.853rem;
    margin-top: 0.16rem;
    font-size:0.4rem;
    text-align: left;
}
.mui-search .mui-placeholder span{
    padding-left: 0.5rem;
    font-size: 0.34rem;
}
/* =======================================================自定义popver 底部弹出框 =======================================================*/
.mui-popover.custom-popover .mui-card{
    box-shadow: 0 0 0;
}
.mui-popover.custom-popover .mui-table-view{
    background: #fff;
    padding: 0 0.4rem;
    color:#444;
    margin: 0;   
}

.mui-popover.custom-popover .mui-card{
    margin: 0;
}
.mui-popover.custom-popover .mui-card-header {
    font-size: 0.453rem;
    padding: 0.4rem 0.16rem;
    font-weight: 700;
}
.mui-popover.custom-popover .mui-card-header .mui-icon{
    position: absolute;
    right: 0.16rem;
    font-size: 0.9rem;
    font-weight: 700;
}
.mui-popover.custom-popover .mui-card-header:after{
    background-color: #f5f5f5;
}
.mui-popover.custom-popover .mui-table-view.mui-grid-view .mui-table-view-cell{
    display: block;
}
.mui-popover.custom-popover .mui-card-content-inner{
    padding: 0;
    overflow: hidden;
}
.mui-popover.custom-popover .mui-card-content-inner .content-wrapper{
    max-height: 10rem;
    min-height: 5.3rem;
    overflow-y: auto;
}


.mui-popover.custom-popover .mui-scroll-wrapper,
.mui-popover.custom-popover.mui-popover.custom-popover-action .mui-table-view,
.mui-popover.custom-popover.mui-popover.custom-popover-action .mui-table-view.mui-grid-view .mui-table-view-cell{
    margin: 0;
}

/* ========================================================== 列表 宽占全屏 ========================================================== */

.custom-recommend-title {
    text-align: center;
    position: relative;
    font-size: 0.4rem;
    color: #000;
}

.custom-recommend-title img {
    width: 2.48rem !important;
    height: 0.48rem;
}

.custom-recommend-title h3 {
    font-size: 0.46rem;
    font-weight: 700;
}

.custom-recommend-title.custom-recommend-title p {
    height: 0.28rem;
    margin-top: 7px;
    line-height: 0.28rem;
    font-size: 0.28rem;
    color: #adadad;
}

.custom-recommend-title.custom-title-line:before,
.custom-recommend-title.custom-title-line:after {
    display: block;
    content: '';
    position: absolute;
    top: 50%;
    width: 8%;
    height: 0;
    left: 60%;
    border-top: 1px solid #000;
}

.custom-recommend-title.custom-title-line:after {
    left: 32%;
}

.custom-full-width-list-box {
    width: 100%;
    box-sizing: border-box;
}


.custom-full-width-list-box .custom-brand-item:first-child {
    padding-top: 0;
}

.custom-full-width-list-box .custom-brand-item:last-child {
    border-bottom: 0;
}

.custom-brand-item {
    padding-bottom: 0.7rem;
}


.custom-brand-item .custom-brand-info {
    width: 9.16rem;
    position: relative;
    margin-top: 0.24rem;
    margin-left: auto;
    margin-right: auto;
}



.custom-brand-info .custom-brand-desc {
    height: 0.44rem;
    line-height: 0.45rem;
    font-size: 0.36rem;
}

.custom-brand-info .custom-brand-price {
    margin-top: 0.2rem;
    font-size: 0.46rem;
    font-weight: 700;
    color:#000;
}

.custom-brand-price span {
    font-size: 0.5rem;
    color:#1d1d1d;
}

.custom-brand-info .custom-brand-num,
.custom-brand-info .custom-action {
    position: absolute;
}

.custom-brand-info button {
    position: absolute;
    right: 0;
    bottom: 0.08rem;
    width: 1.6rem;
    height: 0.666rem;
    line-height: 0.42rem; 
}

/*** 单个商品  grid ***/
.custom-cell{
    width: 2.93rem;
    /* height: 4.93rem; */
    padding: 0 !important;
    background-color: #fff !important;
}
.custom-cell:nth-child(3n){
    margin-right: 0 !important;
}
.custom-cell .custom-cell-content{
    font-size: 0.346rem;
}
.custom-cell .custom-cell-content
.custom-cell .describe{
    font-size: 0.346rem;
}
.custom-list-box .monthly-price,
.custom-cell .custom-cell-content .monthly-price{
    font-family: MarkPro-Bold;
}
.custom-cell .custom-cell-content small.scale{
    font-size:0.24rem;
}
.custom-list-box .custom-pink small,
.custom-cell .custom-cell-content small{
    margin-right: 0.04rem;
}
.custom-cell .custom-cell-content small:nth-of-type(2){
    margin-left: 0.3rem;
    color: #a6a6a6;
}
.custom-list-box .custom-pink,
.custom-cell .custom-cell-content .custom-pink{
    font-size: 0.346rem;
    position: relative;
}
.mode-2 .custom-cell .custom-cell-content .custom-pink small:nth-of-type(2) {
    position: absolute;
    top: 0.06rem;
    right: 0;
}
.custom-list-box .custom-pink small,
.custom-cell .custom-cell-content .custom-pink small:first-child{
    margin-left: 0.08rem;
    font-weight: 700;
}
.custom-list-box .custom-pink span,
.custom-cell .custom-cell-content .custom-pink span{
    margin-right: 0.1rem;
    line-height: 1;
}
.custom-cell .custom-cell-content .monthly-price{
    display: inline-block;
    line-height: 1;
    position: relative;
    top: 0.02rem;
}
.custom-list-box .position-bottom{
    bottom: 0.26rem;
    width: 85%;
}


/*** 单个商品  cell横向 ***/

.mode2-box .custom-list-pic {
    margin-right: 0.293rem;
}
.mode2-box .custom-brand-info{
    width: calc(10rem - 3.6rem);
}
.mode2-box .custom-list-edit .custom-brand-info{
    width: calc(10rem - 4rem);
}
.mode2-box .custom-list-box .custom-brand-info h3{
    line-height: 1.3;
}
.mode2-box .custom-brand-info h3+p{
    margin:0.1rem 0;
}
.mode2-box .custom-brand-info .badges-box{   
    padding-bottom: 0.24rem;
}
.mode2-box .custom-brand-info .badges-box .mui-badge{                
    line-height: 12px;
}

/*** 一行显示2个 ***/

.mode-2.mui-table-view:before, .mode-2.mui-table-view:after{
    background-color: transparent;
}
.mode-2.mui-table-view.mui-grid-view{
    padding: 0;
}
.mode-2 .custom-cell{
    width: 4.933rem;
    /* height: auto; */
    height: 7.8rem;
}
.mode-2.mui-table-view.mui-grid-view .mui-table-view-cell.custom-cell{
    text-align: left;   
    margin-bottom: 0.13rem; 
}
.mode-2.mui-table-view.mui-grid-view .mui-table-view-cell.custom-cell .mui-media-object{
    padding: 0.106rem 0.266rem 0.133rem 0.173rem;
}
.mode-2 .custom-cell:nth-child(2n){
    float:right;
}
.mode-2 .custom-cell .custom-cell-content{
    padding:0.24rem 0.26rem 0.24rem 0.34rem;

}
.mode-2 .custom-cell .custom-cell-content .pos-bottom{
    position: absolute;
    bottom: 0;
    right: 0.26rem;
    left: 0.34rem;
}
.mode-2 .custom-cell .custom-cell-content .describe{
    padding-right: 0.13rem;
    color:#222;
    font-size: 0.373rem;
    line-height: 1.3;
}
.mode-2 .custom-cell .custom-cell-content .custom-pink small:nth-of-type(2){
    font-size: 0.293rem;
}
.mode-2 .custom-gray{
    margin-top:0.2rem;
    margin-bottom: 0.16rem;
     
}

/*** 推荐 ***/
.mode-2.recommend-box .custom-gray{
    margin-top: 0;
    margin-bottom: 0.1rem;
}

.recommend-box .custom-light-gray{
    float:right;
}
