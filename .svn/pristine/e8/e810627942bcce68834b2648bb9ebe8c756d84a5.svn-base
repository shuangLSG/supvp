/* ================================================ 列表导航 ======================================*/

.custom-list-swipe-wrapper .mui-table-view:after,
.custom-list-swipe-wrapper .mui-table-view:before{
    background-color: #f5f5f5 !important;
}
.custom-list-wrapper .mui-table-view:after,
.custom-list-wrapper .mui-table-view:before {
    background-color: #fff;
}

.custom-list-swipe-wrapper li,
.custom-list-wrapper li {
    font-size: 0.4rem;
    padding: 0.4rem 0.346rem !important;
}



.custom-list-wrapper li a {
    font-size: 0.426rem;
    margin: -0.34rem -0.2rem !important;
    margin-right: -0.5rem !important;
}

.mui-navigate-right .mui-pull-right {
    margin-right: 0.64rem;
    font-size: 0.346rem;
    color: #A6A6A6;
}

.mui-table-view-cell.mui-active,
.mui-table-view-cell.mui-active>.mui-slider-handle,
.mui-table-view-cell>a:not(.mui-btn).mui-active {
    background-color: transparent !important;
}



/* 带input控件的列表 */

.mui-checkbox input[type=checkbox]:checked:before,
.mui-radio input[type=radio]:checked:before {
    color: #FF7E00;
}

/* checkbox */

.custom-pay-bar .mui-checkbox input[type=checkbox] {
    position: initial;
}

.custom-pay-bar .mui-checkbox input[type=checkbox]:before {
    font-size: 0.6rem;
}

/* ========================================================= 卡片 =============================================*/

.custom-card-header {
    height: 1.133rem;
    padding: 0 0.4rem;
    line-height: 1.133rem;
    font-size: 0.34rem;
    overflow: hidden;
    border-bottom:1px solid #eee;
}

.custom-card-header input {
    vertical-align: middle;
}

.custom-card-header .right-arrow {
    height: 0.32rem;
    width: 0.21rem !important;
    margin-top: -0.1rem;
    margin-left: 0.2rem;
    vertical-align: middle;
}

.custom-card-header,
.custom-card-content {
    position: relative;
    overflow: hidden;
}

.custom-line:after,
.custom-card-header.custom-line:after,
.custom-card-content.custom-line:after {
    position: absolute;
    right: 0;
    bottom: 0;
    left: 15px;
    height: 1px;
    content: '';
    background-color: #ebebeb;
}

.custom-card-footer {
    text-align: right;
    padding: 0.3rem 0.45rem 0.4rem;
}
.custom-card-footer strong{
    font-size:0.4rem;
}
.custom-card-footer p {
    color: #000;
    font-size: 0.32rem;
    margin-bottom: 0.2rem;
}

.custom-card-item .custom-list-pic {
    width: 2.4rem !important;
    height: 2.4rem;
    margin-right: 0.4rem;
}

.custom-card-item li{
    padding: 0.24rem 0.346rem !important;
}
.custom-card-item .custom-brand-info h3+p {
    margin: 0.1rem 0;
}
.custom-card-item .custom-brand-info .badges-box {
    padding-bottom: 0.2rem;
}
/* =================================================  popup 消息框  ===================================================*/
.mui-popup.mui-popup-in{
    border-radius: 0.106rem;
}
.mui-popup-inner,
.mui-popup .mui-popup-button{
    border-radius: 0;
}
.mui-popup-inner{
    padding: 0.64rem 0.7rem 0.6rem;
}
.mui-popup-buttons{
    height: 1.386rem;
}
.mui-popup .mui-popup-button{
    height: 1.386rem;
    line-height: 1.386rem;
    color: #666666;
    font-size: 0.4rem;
}
.mui-popup .mui-popup-button-bold{
    color: #FF7E00;
}

/* ========================================================== dialog 提示框 ==================================================*/

.mui-popup-title {
    padding: 0.1rem 0;
    font-size: 0.426rem;
    color: #444;
}
.mui-popup-title+.mui-popup-text{
    margin: 0.2rem 0 0;
    color:#A6A6A6;
    font-size: 0.346rem;
}
.mui-popup-button:first-child {
    border-right: 1px solid #f5f5f5;
}

.mui-popup-button.mui-popup-button-bold {
    font-weight: normal;
}

/* ===========================================================底部导航 =====================================================*/

.mui-bar-tab {
    height: 1.33rem;
    background-color: #fafafa;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
}
.mui-bar-tab .mui-tab-item{
    color:#6A6A6A;
}
.mui-bar-tab .mui-tab-item.mui-active{
    color:#FFAA00; 
}
/* .mui-bar, */

.mui-bar-tab .mui-tab-item {
    height: 1.33rem;
}

.custom-pay-bar.mui-bar {
    background-color: #fafafa;
    box-shadow: -4px 0 5px rgba(0, 0, 0, .2);
    z-index: 11;
}

.custom-pay-bar.mui-bar {
    line-height: 1.33rem;
    padding-left: 0.36rem;
    font-size: 0.38rem;
}

.custom-pay-bar.mui-bar a:active {
    color: #fff;
}

/****** 暂无数据 ****/
.page-nodata{
    padding-top: 3rem;
}

.custom-empty img {
    width: 5rem !important;
}
.page-nodata img{
    width: 3rem;
}
.page-nodata p,
.custom-empty p {
    margin-top: 0.6rem;
    font-size: 0.34rem;
}


/* ============================================================= 分页器 ==========================================*/

.mui-slider-indicator .mui-indicator{
    width: 9px;
    height: 3px;
    background: rgba(255,255,255,0.6);
    border-radius: 3px;
    box-shadow:0 0 0 rgba(0,0,0,0) !important;
    border-radius: 999px;
    margin: 0.01rem 0.08rem; 
}
.mui-slider-indicator .mui-active.mui-indicator{
    background: rgba(255,255,255,1);
}
.mui-slider-group .mui-scroll-wrapper{
    padding-bottom:2.9rem !important;
}

/* ============================================================= 数字输入框控件 ==========================================*/

.mui-numbox {
    height: 0.586rem;
    padding: 0 0.586rem;
    width: 2.186rem;
    border: none;
}

.mui-numbox .tep-reduce{
    width: 0.72rem !important;
}
.mui-numbox [class*=btn-numbox],
.mui-numbox [class*=numbox-btn] {
    width: 0.69rem;
    line-height: 5%;
    color: #646464;
}

.mui-numbox [class*=btn-numbox],
.mui-numbox [class*=numbox-btn],
.mui-numbox [class*=btn-numbox]:active,
.mui-numbox [class*=numbox-btn]:active {
    background-color: #fff !important;
}

.mui-numbox .mui-numbox-input {
    width: 0.8rem;
    color: #222;
    line-height: initial;
    background-color: #F7F7FA !important;
}
.custom-slider-group {
    position: relative;
    margin-top: 1.25rem;
}


.mui-numbox [class*=numbox-btn][disabled],
.mui-numbox [class*=numbox-btn][disabled]:active{
    background: url(../images/icons/numbox_0.png) no-repeat;
    background-size: 100%;
    opacity: 1;
    color:#ccc;
}
.mui-numbox .tep-reduce,
.mui-numbox [class*=numbox-btn].tep-reduce:active{
    background: url(../images/icons/numbox_1.png) no-repeat;
    background-size: 100%;
}
.mui-numbox .tep-plus,
.mui-numbox [class*=numbox-btn].tep-plus:active{
    background: url(../images/icons/numbox_2.png) no-repeat;
    background-size: 100%;
}

/* ======================================================= toast信息提示 =================================================*/

.mui-toast-container {
    bottom: 50% !important;
}

.mui-toast-message{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-content: center;
    font-size: 0.4rem;

}
.mui-toast-message .custom-toast-icon{
    width: 1.4rem;
    height: 1.4rem;
    margin: auto;
    margin-bottom: 0.3rem;
}


/*  =================================================    view    =================================================*/



.mui-navbar {
    position: fixed;
    right: 0;
    left: 0;
    z-index: 10;
    height: 44px;
    background-color: #fff;
}

.mui-navbar .mui-bar {
    position: absolute;
    background: transparent;
    text-align: center;
}
.mui-navbar .mui-bar-nav{
    box-shadow: 0 0 0 rgba(0, 0, 0, 0) !important;

}

.mui-android .mui-navbar-inner.mui-navbar-left {
    opacity: 0;
}

.mui-ios .mui-navbar-left .mui-left,
.mui-ios .mui-navbar-left .mui-center,
.mui-ios .mui-navbar-left .mui-right {
    opacity: 0;
}

.mui-navbar .mui-btn-nav {
    -webkit-transition: none;
    transition: none;
    -webkit-transition-duration: .0s;
    transition-duration: .0s;
}

.mui-navbar .mui-bar .mui-title {
    display: inline-block;
    width: auto;
}

.mui-page-shadow {
    position: absolute;
    right: 100%;
    top: 0;
    width: 16px;
    height: 100%;
    z-index: -1;
    content: '';
}

.mui-page-shadow {
    background: -webkit-linear-gradient(left, rgba(0, 0, 0, 0) 0, rgba(0, 0, 0, 0) 10%, rgba(0, 0, 0, .01) 50%, rgba(0, 0, 0, .2) 100%);
    background: linear-gradient(to right, rgba(0, 0, 0, 0) 0, rgba(0, 0, 0, 0) 10%, rgba(0, 0, 0, .01) 50%, rgba(0, 0, 0, .2) 100%);
}


/* ================================================================= 方格 ===================================================*/
.custom-box-title{
    height:1rem;
    line-height: 1rem;
    font-size:0.4rem;
    color:#222;
}
.custom-box-title p{
    padding-left: 0.3rem;
    font-size: 0.38rem;
}


/*弹出框*/
.mui-poppicker-btn-ok{
    background: #d54843 !important;
    border:1px solid #d54843 !important;
}

/*分页器*/
.mui-slider-indicator .mui-indicator {
    box-shadow: 0 0 0 rgba(0,0,0,0) !important;
}



/* 容器 */
.has-foot-bar{
    padding-bottom:1.33rem;
}


/*弹出框*/
.mui-popup.copy-popup{
    background-color: #fff;
}
.copy-popup .mui-popup-title {
    font-size: 0.34rem;
    color: #6d6d6d;
}
.mui-popup.copy-popup .mui-popup-button-bold{
    border: none;
    background-color: #fff !important;
}
.mui-popup.copy-popup .mui-popup-button{
    color:#000;
}



.mui-pull-caption{
    color: #A6A6A6;
    font-size: 0.34rem;
}