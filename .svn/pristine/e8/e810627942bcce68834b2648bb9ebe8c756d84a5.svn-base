body {
    background: #fff;
}

img {
    vertical-align: top;
    /* pointer-events: none;    Android 机上禁止点击图片跳出来 */
}


a {
    color: #000;
}
a:hover,
a:active {
    text-decoration: none !important;
}
small,.font-Arial{
    font-family:Arial;
}
a:active {
    color: #000;
}

@font-face {
    font-family: MarkPro-Bold;
    src: url("../fonts/MarkPro-Bold.otf");
}
.font-MarkPro{
    font-family: MarkPro-Bold;
}
/* mui */

input::-webkit-input-placeholder {
    color: #c0c0c0;
}

button:enabled:active {
    color: #000 !important;
    background: transparent;
    
}
.mui-btn-link:enabled:active{
    border: none !important;
}


.mui-input-group:before,
.mui-input-group:after,
.mui-table-view:before,
.mui-table-view:after {
    background-color: #f5f5f5;
}

.mui-input-group .mui-input-row:after,
.mui-table-view-cell:after {
    background-color: #ebebeb;
}

.mui-navigate-right:after {
    font-size: 0.5rem;
    top: 50%;
}

.mui-checkbox input[type=checkbox]:before,
.mui-radio input[type=radio]:before {
    font-size: 0.6rem;
    color: #D5D5D5;
}

.mui-checkbox input[type=checkbox] {
    position: initial;
    width: 0.56rem;
    height: 0.56rem;
}

.mui-badge-red,
.mui-badge-warning{
    background-color: #FF7E00;
    border-radius:2px;
    padding: 0 0.12rem;
}
.mui-badge-outlined {
    background-color: transparent;
}
.mui-btn-warning.mui-btn-outlined:enabled:active,
.mui-btn-warning.mui-btn-outlined,
.mui-badge-warning.mui-badge-outlined{
    color:#FF7E00;
    border:1px solid #FF7E00;
    border-radius: 99px;
    padding: 0 0.16rem 0 !important;
    /* width: 0.733rem;
    height: 0.346rem; */
    padding: 0;
    text-align: center;
    line-height: 12px;
}
.badge-icon{
    width: 0.8rem;
    margin-top: 0.1rem;
}
.mui-btn-warning.mui-btn-outlined:enabled:active{
    color:#FF7E00 !important;
    background-color: transparent !important;
}
.mui-badge-red{
    background-color:#f22d47;
}
.custom-orange-gradient {
    background-color: transparent;
    background: -webkit-linear-gradient(left, #FFD225 , #FFB410);
    background: -o-linear-gradient(right, #FFD225 , #FFB410);
    background: linear-gradient(to right, #FFD225 , #FFB410);
}
button.custom-carmine-gradient:enabled:active,
.custom-carmine-gradient {
    color:#fff !important  ;
    background-color: transparent;
    background: -webkit-linear-gradient(left, #FF7200 , #FF4605);
    background: -o-linear-gradient(right, #FF7200 , #FF4605);
    background: linear-gradient(to right, #FF7200 , #FF4605);
}
button.custom-btn-gradient:enabled:active,
.custom-btn-gradient{
    background-color: transparent;
    background: -webkit-linear-gradient(left, #F85921 , #FFA200);
    background: -o-linear-gradient(right, #F85921 , #FFA200);
    background: linear-gradient(to right, #F85921 , #FFA200);
}
.mui-center{
    text-align: center;
}

.custom-around{
    border-radius:50%;
}
/* ======================================================== 按钮 ====================================================*/

.custom-radius {
    border-radius: 999px;
}

.custom-btn,
.custom-danger-btn,
.custom-default-btn {
    line-height: 0.42rem;
    height: 0.666rem;
    padding: 0.1rem 0.3rem;
    background-color: transparent;
}


.custom-black-btn {
    color: #fff;
    border: none;
}

.custom-pay-btn {
    width: 3.14rem;
    color: #fff;
    text-align: center;
    background-color: #E70E1D;
}

.custom-default-btn {
    border: 1px solid #c1c1c1;
    color: #000;
}
.custom-default-btn-fill {
    background-color: #cd9e61 !important;
    color: #fff;
    border: none;
}
.custom-light-wraning-fill:enabled:active,
.custom-light-wraning-fill {
    background-color: #FFAA00 !important;
    color: #fff !important;
    border: none;
}

.custom-danger-btn {
    border: 1px solid #E70E1D;
    color: #E70E1D;
}

.custom-disable-btn {
    color: #fff !important;
    background-color: #bfbdbd !important;
}

.custom-block-btn {
    display: block;
    width: 100%;
    height: 1.46rem;
    line-height: 1.46rem;
    text-align: center;
    color: #fff;
    font-size: 0.42rem;
    border-radius: 4px;
    margin-left: auto;
    margin-right: auto;
    background-color: #b89e6e;
}

a.custom-block-btn:active {
    color: #fff;
}

button.custom-danger-btn:enabled:active {
    color: #d54843;
}
.text-decoration{
    text-decoration: line-through;
}


.wh-180{
    width:2.4rem !important;
    height: 2.4rem !important;
}
/* ============================================================ 文本溢出 ==========================================*/
.mui-popover.mui-popover-action .mui-table-view .mui-table-view-cell:after{
    background-color: #eee;
}
.custom-white-space {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
}
.custom-white-space-clamp,
.mode-2 .custom-cell .custom-cell-content .describe{
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}


.custom-padding-top-28 {
    padding-top: 0.37rem;
}

.custom-margin-top-16 {
    margin-top: 0.2rem;
}
.custom-margin-bottom-16 {
    margin-bottom: 0.2rem;
}
.custom-margin-top-20 {
    margin-top: 0.3rem;
}
.custom-margin-top-30 {
    margin-top: 0.4rem;
}


.custom-bg-color-gray {
    background-color: rgba(249,249,252,1);
}
.custom-margin-right-16{
    margin-right: 0.2rem;
}
.custom-bg-color-white {
    background-color: #fff !important;
}
.page-bg{
    background-color: #F9F9FC;
}
.custom-bg-light-gray{
    background-color: #D0D0D0;
}
.custom-red{
    background-color: #E70E1D !important;
}



