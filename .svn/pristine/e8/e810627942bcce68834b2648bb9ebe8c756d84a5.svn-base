}

 .custom-refunds-status h3 {
    color: #fff;
}

/* ======================================================= 商品列表 ===================================================*/

.custom-list-box {
    width: 100%;
}

.custom-list-box .custom-list-pic {
    margin-right: 0.4rem;
}

.custom-list-box .custom-brand-info {
    width: calc(10rem - 5rem);
    flex: 1;   
}
.custom-list-box .custom-brand-info h3 {
    font-size: 0.373rem;
    line-height: 1.5;
    color: #292929;
}
.custom-list-box .custom-brand-info .custom-brand-desc {
    margin-bottom: 0.2rem;
    margin-top: 0.1rem;
