.page-nodata {
    position: fixed;
    top:  0.95rem;
    bottom: 0;
    right: 0;
    left: 0;
    z-index: 99;
    background-color: #fff;
}

.mui-segmented-control .mui-control-item {
    line-height: 0.94rem;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
    border-bottom: none !important;
}

.mui-segmented-control .mui-control-item span {
    display: inline-block;
    font-size: 0.4rem;
    color: #888;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active span {
    color: #FF7E00 !important;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active span:after {
    content: '';
    display: block;
    margin-top: -3px;
    height: 3px;
    background: #FF7E00;
}
.mui-control-wrapper {
    position: fixed;
    bottom: 0;
    top: 0.94rem;
    left: 0;
    right: 0;
    border-top: 1px solid #eee !important;
}

.mui-control-content {
    height: 100% !important;
}


.mui-table-view:before,
.mui-table-view:after {
    height: 0;
}

.mui-table-view-cell:after {
    left: 0;
}
.coupon-box .mui-table-view{
    padding-top: 0.1rem;
}
.coupon-box .mui-table-view small {
    color: #444;
}
.coupon-box .mui-table-view .mui-table-view-cell {
    height: 2.346rem;
}
.expired .custom-pink,
.expired p {
    color: #a6a6a6;
}

