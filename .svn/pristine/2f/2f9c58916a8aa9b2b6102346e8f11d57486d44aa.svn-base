.mui-scroll-wrapper{
    background: #f5f5f5;
}
.mui-scroll .mui-control-item{
    display: block;
    height:1.4rem;
    line-height: 1.4rem;
    text-align: center;
    color: #000002;
    font-size: 0.36rem;
    box-sizing: border-box;
    border-bottom: 1px solid#dfdfdf;
}
.mui-scroll .mui-control-item.active{
    background-color: #fff;
    border-right: none;
}
.custom-control-content .mui-scroll-wrapper {
    background-color: #fff;
}
.custom-control-content ol{
    overflow: hidden;
    padding-bottom: 0.26rem;
}
.custom-control-content li{
    float: left;
}
.custom-control-content li img{
    width: 2.1rem;
    height: 2rem;
    margin-left: 0.3rem;
    margin-top: 0.26rem;
}
